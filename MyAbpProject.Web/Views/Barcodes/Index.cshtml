@using Abp.Web.Mvc.Extensions
@model Abp.Application.Services.Dto.PagedResultDto<MyAbpProject.Barcodes.Dtos.BarcodeDto>
@{
    ViewBag.ActiveMenu = "Barcodes";
}
@section scripts
{
    @Html.IncludeScript("~/Views/Barcodes/Index.js")
}
<div>
    <h1>@L("Users")</h1>
    <div class="row">
        <div class="col-md-12">
            <button data-toggle="modal" data-target="#UserCreateModal" class="btn btn-primary pull-right"><i class="fa fa-plus"></i> @L("CreateNewUser")</button>
            <table class="table">
                <thead>
                    <tr>
                        <th>@L("Id")</th>
                        <th>@L("SerialNumber")</th>
                        <th>@L("CreationTime")</th>
                        <th>@L("Description")</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var barcode in Model.Items)
                    {
                        <tr>
                            <td>@barcode.Id</td>
                            <td>@barcode.SerialNumber</td>
                            <td>@barcode.CreationTime</td>
                            <td>@barcode.Description</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

